public class BoxVolume {
    public static void main(String[] args) {
        // Box 1 dimensions
        double height1 = 10.5;
        double width1 = 7.2;
        double depth1 = 4.8;

        // Calculate volume for Box 1
        double volume1 = height1 * width1 * depth1;

        // Box 2 dimensions
        double height2 = 5.0;
        double width2 = 6.5;
        double depth2 = 3.5;

        // Calculate volume for Box 2
        double volume2 = height2 * width2 * depth2;

        // Display results
        System.out.println("Box 1 Dimensions:");
        System.out.println("Height: " + height1);
        System.out.println("Width: " + width1);
        System.out.println("Depth: " + depth1);
        System.out.println("Volume of Box 1: " + volume1);
        System.out.println();

        System.out.println("Box 2 Dimensions:");
        System.out.println("Height: " + height2);
        System.out.println("Width: " + width2);
        System.out.println("Depth: " + depth2);
        System.out.println("Volume of Box 2: " + volume2);
    }
}
